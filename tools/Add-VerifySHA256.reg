Windows Registry Editor Version 5.00

; Adds "Verify SHA-256..." to the right-click menu for all files
[HKEY_CURRENT_USER\Software\Classes\*\shell\VerifySHA256]
"MUIVerb"="Verify SHA-256..."
; Use the EXE's embedded icon (recommended)
"Icon"="C:\\_Local_Dev_Ops\\Tools\\MizzleVerify.exe,0"

[HKEY_CURRENT_USER\Software\Classes\*\shell\VerifySHA256\command]
@="\"C:\\_Local_Dev_Ops\\Tools\\MizzleVerify.exe\" \"%1\""
